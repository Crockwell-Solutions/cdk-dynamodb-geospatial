{
  "name": "dynamodb-geospatial-demo",
  "version": "0.1.0",
  "bin": {
    "dynamodb-geospatial-demo": "bin/dynamodb-geospatial-backend.ts"
  },
  "scripts": {
    "build": "tsc",
    "build:esbuild": "node_modules/.bin/esbuild --bundle src/**/*.ts --outbase=src/ --outdir=dist/ --sourcemap=inline --platform=node",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth --all",
    "deploy": "npm run deploy:backend && npm run deploy:frontend",
    "deploy:backend": "cdk deploy --all -O frontend/src/cdk-output.json --require-approval never",
    "deploy:frontend": "npm --prefix frontend run build && cdk deploy --app='npx ts-node --prefer-ts-exts bin/dynamodb-geospatial-frontend.ts' --require-approval never",
    "destroy": "npm run destroy:frontend && npm run destroy:backend",
    "destroy:backend": "cdk destroy --all",
    "destroy:frontend": "cdk destroy --app='npx ts-node --prefer-ts-exts bin/dynamodb-geospatial-frontend.ts'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "jest",
    "preprocess": "ts-node resources/seed-data/preprocess-data.ts"
  },
  "devDependencies": {
    "@aws-lambda-powertools/logger": "^2.25.2",
    "@aws-sdk/client-dynamodb": "^3.878.0",
    "@aws-sdk/client-s3": "^3.878.0",
    "@aws-sdk/client-ssm": "^3.876.0",
    "@aws-sdk/lib-dynamodb": "^3.878.0",
    "@aws-sdk/util-dynamodb": "^3.878.0",
    "@eslint/js": "^9.34.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^30.0.0",
    "@types/ngeohash": "^0.6.8",
    "@types/node": "24.3.0",
    "@types/proj4": "^2.19.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/parser": "^8.41.0",
    "aws-sdk-client-mock": "^4.1.0",
    "axios": "^1.11.0",
    "cdk-nag": "^2.37.12",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "geolib": "^3.3.4",
    "geotiff": "^2.1.4-beta.0",
    "globals": "^16.3.0",
    "jest": "^30.1.1",
    "ngeohash": "^0.6.3",
    "prettier": "^3.6.2",
    "proj4": "^2.19.10",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.41.0",
    "ulid": "^3.0.1",
    "xml2js": "^0.6.2"
  },
  "dependencies": {
    "aws-cdk": "2.1027.0",
    "aws-cdk-lib": "2.213.0",
    "constructs": "^10.4.2"
  }
}
